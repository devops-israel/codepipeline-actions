{
  "compilerOptions": {
    "strictNullChecks": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "target": "es5",
    "outDir": "./dist/",
    "lib": ["es2015"],
    "moduleResolution": "node"
  },
  "exclude": [
    "codepipeline-shared",
    "codepipeline-slack",
    "codepipeline-auto",
    "**/*.test.ts",
    "node_modules",
    "layers",
    "dist",
    ".vscode",
    ".serverless"
  ]
}
